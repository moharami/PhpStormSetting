<templateSet group="Cakephp">
  <template name="d" value="debug($this$);&#10;die();" toReformat="false" toShortenFQNames="true">
    <variable name="this" expression="" defaultValue="this-&gt;request-&gt;data" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="f" value="public function $name$(){&#10;    $value$&#10;}" description="create function" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="value" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="func" value="public function $name${&#10;$value$&#10;}" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="value" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="_" value="__d('Behandam',$message$);" description="multilanguage" toReformat="false" toShortenFQNames="true">
    <variable name="message" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="tag" value="$this-&gt;Html-&gt;tag($name$, $text$ , $options$);" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="capitalize(String)" defaultValue="$name" alwaysStopAt="true" />
    <variable name="text" expression="escapeString(String)" defaultValue="text" alwaysStopAt="true" />
    <variable name="options" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="msgid" value="msgid &quot;$selection$&quot;&#10;msgstr &quot;&quot;&#10;" description="msgid for po file" toReformat="false" toShortenFQNames="true">
    <variable name="selection" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="dd" value="debug($this-&gt;request-&gt;data);&#10;die();" description="debug($this-&gt;request-&gt;data);" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="l" value="$d = $this-&gt;FoodList-&gt;getDataSource()-&gt;getLog(false,false);&#10;debug($d);&#10;die();" description="log Query" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>